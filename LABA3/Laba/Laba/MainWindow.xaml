<Window x:Class="Laba.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Laba"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Content="Task1" Grid.Row="0" Name ="Button1"/>
        <Button Content="Task2" Grid.Row="1" Name="Button2"/>
        <Button Content="Task3" Grid.Row="2" Name ="Button3"/>
        <Button Content="Task4" Grid.Row="3" Name ="Button4"/>
        <Button Content="Task5" Grid.Row="4" Name ="Button5"/>
        <Button Content="Task6" Grid.Row="5" Name ="Button6"/>
    </Grid>
</Window>
